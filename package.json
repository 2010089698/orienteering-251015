{
  "name": "orienteering-monorepo",
  "private": true,
  "scripts": {
    "dev:frontend": "npm run dev --workspace @orienteering/startlist-frontend",
    "dev:startlist-backend": "npm run dev:backend --workspace startlist-management",
    "dev:entry-backend": "npm run start --workspace @entry-management/backend",
    "dev:event-backend": "npm run start --workspace @event-management/backend"
  },
  "workspaces": [
    "StartlistManagement",
    "apps/*",
    "StartlistManagement/packages/*",
    "StartlistManagement/apps/*",
    "EntryManagement/packages/*",
    "EventManagement",
    "EventManagement/packages/*",
    "EventManagement/apps/*"
  ]
}
